<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="FWA.Logic"
                   namespace="FWA.Logic.Storage">

  <class name="Check" table="checks">
    <id name="ID" column="CheckID">
      <generator class="native"/>
    </id>
    <property name="Name" column="CheckName" />
    <property name="InvNumber" column="CheckInvNumber" />
    <property name="StringDate" column="CheckDate" />
    <property name="WhoChecked" column="DeviceUser" />
    <property name="CheckType" column="CheckTypeID" />
    <property name="Lack" column="DeviceLack" />
    <property name="Comment" column="DeviceComment" />
  </class>

</hibernate-mapping>