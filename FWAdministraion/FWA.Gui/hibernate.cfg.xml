      <property name="connection.driver_class">NHibernate.Driver.MySqlDataDriver</property>
      <property name="connection.connection_string">Server=127.0.0.1;Database=fwadministration;Uid=root;Pwd=volks3wagen;</property>
      <property name="show_sql">true</property>
      <mapping assembly="FWA.Logic" />
    </session-factory>
  </hibernate-configuration>
</configuration>